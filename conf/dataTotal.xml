<?xml version="1.0"?>
<config>
        <!-- spark应用名称  -->
        <appName>DDCDataCollection</appName>
        <!-- 每隔多少时间从kafka获取数据，单位秒 -->
        <duration>10</duration>
        <!-- kafka集群地址  -->
        <brokers>10.10.4.85:9092,10.10.10.21:9092</brokers>
        <!-- kafka消费团体  -->
        <group>DataCollectionGroup</group>
        <!-- kafka订阅主题  -->
        <topic>TagTopic</topic>
        <!-- kafka消费偏移量方式  -->
        <offset>latest</offset>
        <!-- 设备采集实时统计表(分)  -->
        <devMinuteTotalTable>DDCDevMinuteTotal</devMinuteTotalTable>
        <!-- 设备采集实时统计表(小时)  -->
        <devHourTotalTable>DDCDevHourTotal</devHourTotalTable>
        <!-- 设备采集实时统计表(日)  -->
        <devDayTotalTable>DDCDevDayTotal</devDayTotalTable>
        <!-- 设备采集实时统计表(月)  -->
        <devMonthTotalTable>DDCDevMonthTotal</devMonthTotalTable>
        <!-- redis host -->
        <redisHost>10.10.4.85</redisHost>
        <!-- redis port -->
        <redisPort>6379</redisPort>
        <!-- redis timeout -->
        <redisTimeout>2000</redisTimeout>
        <!-- kafka redis key -->
        <kafkaOffsetKey>kafkaOffset_dataCollection</kafkaOffsetKey>
        <!-- total redis key -->
        <totalKey>DDCCollectionKey</totalKey>
        <!-- dev total redis key -->
        <devTotalKey>DDCDevCollectionKey</devTotalKey>
        <!-- tag redis key -->
        <tagKey>DDCTagKey</tagKey>
        <!-- tag type 电动车1 人员2 -->
        <tagType>1</tagType>
</config>
